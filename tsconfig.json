{
  "compilerOptions": {
    "target": "ES2021",                                
    "module": "commonjs",                           
    "strict": false,  
    "resolveJsonModule": true,                               
    "baseUrl": "./",                             
   "types": [
     "./node_modules/@types",
     "./src/@types"
   ],                                 
    "esModuleInterop": true,                        
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true        
  }
}
